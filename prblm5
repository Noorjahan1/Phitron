#include <bits/stdc++.h>

using namespace std;


int main()
{
    int n;

    cin>>n;
    vector<int>v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
    }

    for(int i=0;i<n;i++){
        int m=0;
      for(int j=i+1;j<n;j++){
        if(v[j]>m){
            m=v[j];
        }

      }
      cout<<m<<" ";
    }

    return 0;
}

